<h1>Create a new workout</h1>

<%= simple_form_for @workout do |f| %>
<div class="form-inputs">
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :room %>
  </div>


<div class= "form-actions">
  <%= f.button :submit, 'Create Workout' %>
  </div>
<% end %>

<%= link_to 'Back', workouts_path %>
